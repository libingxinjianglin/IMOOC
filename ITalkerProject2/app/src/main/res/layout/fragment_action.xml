<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="italker.tencent.com.italkerproject.fragments.main.ActionFragment">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/contact_recycler"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="false"
        android:elevation="1dp"
        android:outlineProvider="bounds"
        android:paddingBottom="@dimen/len_12"
        android:paddingTop="@dimen/len_12"
        android:visibility="gone"
        tools:targetApi="lollipop" />

    <italker.tencent.com.common.weiget.EmptyView
        android:id="@+id/contact_empty"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</FrameLayout>
